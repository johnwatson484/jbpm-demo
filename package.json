  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "@hapi/crumb": "9.0.0",
    "@hapi/hapi": "20.2.2",
    "@hapi/inert": "7.0.0",
    "@hapi/vision": "7.0.0",
    "hapi-pino": "11.0.1",
    "joi": "17.7.0",
    "nunjucks": "3.2.4"
  },
  "devDependencies": {
    "blipp": "4.0.2",
    "eslint": "9.7.0",
    "jest": "29.2.2",
    "jest-junit": "14.0.1",
    "neostandard": "0.11.1",
    "nodemon": "3.1.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test-output/**"
    ]
